<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:vm="clr-namespace:MusicEco.ViewModels"
             xmlns:page_vm="clr-namespace:MusicEco.ViewModels.Pages"
             xmlns:local="clr-namespace:MusicEco"
             x:Class="MusicEco.App">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Settings/GlobalStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <vm:AppSettingModel x:Key="AppSetting"/>
            <vm:GlobalModel x:Key="GlobalModel"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
